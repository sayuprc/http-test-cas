{
    "name": "sayuprc/http-test-case",
    "description": "Library for HTTP testing",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "phpunit/phpunit": "^10.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.15",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^2.4",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Sayuprc\\HttpTestCase\\": "src/"
        }
    },
    "authors": [
        {
            "name": "sayuprc",
            "email": "mkpoto15@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
